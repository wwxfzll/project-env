{
    "presets": [
        [
            "env",
            {
                "modules": false
            }
        ],
        "stage-2"
    ],
    "plugins": [
        "transform-runtime",
        "transform-vue-jsx",
        "syntax-dynamic-import",
        [
            "component",
            [{"libraryName": "element-ui", "styleLibraryName": "theme-chalk"}]
        ]
    ],
    "comments": false,
    "env": {
        "test": {
            "presets": [
                "env",
                "stage-2"
            ],
            "plugins": [
                "istanbul"
            ]
        }
    }
}
